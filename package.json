  "scripts": {
    "install:yarn": "npm install yarn@1.19.2 -g",
    "tools": "npm run install:yarn",
    "test": "jest --config jestconfig.json --no-cache",
    "pretestc": "rm -rf ./build/coverage",
    "testc": "jest --config jestconfig.json --no-cache --coverage",
    "test:watch": "npm run test -- --watch",
    "prebuild": "rm -rf lib/ && mkdir -p lib",
    "lint": "eslint src --config ./.eslintrc.json --ignore-path ./.eslintignore",
    "validate": "npm run lint && npm run testc",
    "build": "babel src --out-dir lib --copy-files --source-maps",
    "prepublish": "npm run build",
    "lint:fix": "eslint --fix src --config ./.eslintrc.json --ignore-path ./.eslintignore",
    "doc": "react-docgen ./src --ignore node_modules,lib,dist,docs,DEV,coverage,mochawesome-reports,mochawesome-report,webpack.config.js,src/__test__ -e **/*.spec.js --resolver findAllComponentDefinitions --pretty | ./tools/docs/generateDocs.sh",
    "storybook": "start-storybook --port 8080",
    "build:storybook": "build-storybook -c .storybook -o docs"
  }
